{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@core/*": ["./src/app/core/*"],
      "@components/*": ["./src/app/components/*"],
      "@features/*": ["./src/app/features/*"],
      "@env": ["./src/environments/environment.ts"]
    }

  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/*.spec.ts"
  ]
}
